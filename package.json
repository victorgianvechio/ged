    "seed": "sucrase-node prisma/seed.js",
    "dev": "set NODE_ENV=development&& yarn prismix && nodemon src/server.js",
    "prod": "set NODE_ENV=production&& yarn prismix && nodemon dist/server.js",
    "build": "babel src -d dist",
    "eslint:fix": "yarn eslint --fix src --ext .js",
    "test": "jest -i",
    "docker:stop": "docker stop mongo postgres",
    "docker:start": "docker start mongo postgres",
    "docker:restart": "docker restart mongo postgres"
